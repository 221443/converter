{
  "name": "client-side-converter",
  "version": "2.0.0",
  "description": "A powerful, browser-based utility for converting multiple image and PDF files without uploading them to a server",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deploy": "next build && touch out/.nojekyll && gh-pages -d out -t true"
  },
  "keywords": [
    "image-converter",
    "pdf-converter",
    "client-side",
    "browser-based",
    "privacy",
    "batch-conversion",
    "file-processing"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "jspdf": "^2.5.1",
    "jszip": "^3.10.1",
    "next": "^15.5.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.5.5",
    "eslint-config-prettier": "^9.0.0",
    "gh-pages": "^6.0.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0",
    "vitest": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/221443/converter.git"
  },
